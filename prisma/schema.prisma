generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Koordinat {
  x1 Float
  y1 Float
  x2 Float
  y2 Float
  id String @id
  keterangan String
}